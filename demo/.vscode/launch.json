{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "chrome",
      "request": "launch",
      "sourceMaps": true,
      "url": "http://localhost:3535/",
      "webRoot": "${workspaceRoot}/dist",
      "sourceMapPathOverrides": {
        "webpack:///*": "${workspaceRoot}/*"
      }
    },
    {
      "name": "Attach",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "sourceMaps": true,
      "url": "http://localhost:3535/",
      "webRoot": "${workspaceRoot}/dist",
      "sourceMapPathOverrides": {
        "webpack:///*": "${workspaceroot}/*"
      }
    }  
  ]
}
